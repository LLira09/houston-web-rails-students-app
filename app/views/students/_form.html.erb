<%= form_for(@student) do |f| %>
    <%= f.label "Name" %>
    <%= f.text_field :name %>
    <%= f.label "Major" %>
    <%= f.text_field :major %>
    <%= f.label "Age" %>
    <%= f.text_field :age %>
    <%= f.label "Select Instructor" %>
    <%= f.collection_select :instructor_id, @instructor, :id, :name%>

    <%= f.submit %>
<% end %>