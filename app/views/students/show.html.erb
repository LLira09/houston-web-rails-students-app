<h1>Student Details</h1>
<h2>Name: <%= @student.name%></h2>
<p>Major:<%= @student.major%></p>
<p>Age: <%= @student.age%></p>
<p>Instructor: <%= @student.instructor.name%></p>
<p><%= link_to 'Edit', edit_student_path(@student)%></p>
<p><%= link_to 'Delete', student_path(@student), method: :delete, data: { confirm: "Are you sure?"} %></p>